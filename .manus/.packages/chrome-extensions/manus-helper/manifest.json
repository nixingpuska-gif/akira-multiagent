{
  "manifest_version": 3,
  "name": "Manus Helper",
  "version": "1.1.11",
  "description": "Helps Manus to use this browser.",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "logo.png"
  },
  "permissions": [
    "declarativeNetRequest",
    "scripting",
    "webRequest",
    "webNavigation",
    "tabCapture"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/contentInitScript.js"
      ],
      "run_at": "document_start",
      "world": "MAIN"
    },
    {
      "js": [
        "assets/content.ts-loader.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/*",
        "content.ts.js"
      ],
      "use_dynamic_url": false
    }
  ]
}